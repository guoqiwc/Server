<!--50000打头，用户行为埋点-->
<root>

	<!--
	0:打开关闭主界面~
	1:打开关闭悬浮穿~
	2:打开关闭设置面板~
	3:打开关闭网页截图任务窗~
	4:打开关闭关于窗口~
	5:打开关闭帮助窗口~
	6:打开关闭屏幕取色~
	7:打开关闭网页截图色彩分析
	8:打开关闭url监听
	-->
	<sendMessage id="50000" type="CS_SEND_USER_BEHAVIOR" name="CSUserBehaviorRequest" comment="总体界面和状态功能性行为采集">
		<attribute name="type" type="short" comment="
		类型
		0:打开关闭主界面
		1:打开关闭悬浮穿
		2:打开关闭设置面板
		3:打开关闭网页截图任务窗
		4:打开关闭关于窗口
		5:打开关闭帮助窗口
		6:打开关闭屏幕取色
		7:打开关闭网页截图色彩分析
		8:打开关闭url监听
		"
		/>
		<attribute name="state" type="byte" comment="0:关闭，1:打开"/>
		<attribute name="timeStamp" type="longstring" comment="当前本地时间"/>
		<attribute name="macId" type="longstring" comment="用户mac地址"/>
	</sendMessage>
	
	<!--
		0:点击打开文件~
		1:保存项目文件(无图)~
		2:保存项目文件(分析中)~
		3:保存项目文件(有图)~
		4:保存项目文件(保存成功)~
		5:保存图片快照(无图)~
		6:保存图片快照(分析中)~
		7:保存图片快照(有图)~
		8:保存图片快照(保存成功)~
		9:屏幕截图~
		10:开启屏幕取色~
		11:关闭屏幕取色~
		12:开启网页截图分析~
		13:关闭网页截图分析~
		14:开启url监听~
		15:关闭url监听~
		16:打开图片~
		17:打开项目~
		18:点击放大
		19:点击缩小
		20:点击分析(无图)~
		21:点击分析(有图)~
		22:点击删除
		23:点击添加颜色
		24:点击编辑颜色
		25:点击导出aco色板(无色板)~
		26:点击导出aco色板(有色板)~
		27:点击导出aco色板(导出完成)~
		28:点击导出ase色板(无色板)~
		29:点击导出ase色板(有色板)~
		30:点击导出ase色板(导出完成)~
	-->
	<sendMessage id="50002" type="CS_SEND_USER_BEHAVIOR_MAIN_WINDOW" name="CSUserBehaviorMainWindowRequest" comment="主界面用户行为采集">
		<attribute name="type" type="short" comment="
		类型
		0:点击打开文件~
		1:保存项目文件(无图)~
		2:保存项目文件(分析中)~
		3:保存项目文件(有图)~
		4:保存项目文件(保存成功)~
		5:保存图片快照(无图)~
		6:保存图片快照(分析中)~
		7:保存图片快照(有图)~
		8:保存图片快照(保存成功)~
		9:屏幕截图~
		10:开启屏幕取色~
		11:关闭屏幕取色~
		12:开启网页截图分析~
		13:关闭网页截图分析~
		14:开启url监听~
		15:关闭url监听~
		16:打开图片~
		17:打开项目~
		18:点击放大
		19:点击缩小
		20:点击分析(无图)~
		21:点击分析(有图)~
		22:点击删除
		23:点击添加颜色
		24:点击编辑颜色
		25:点击导出aco色板(无色板)~
		26:点击导出aco色板(有色板)~
		27:点击导出aco色板(导出完成)~
		28:点击导出ase色板(无色板)~
		29:点击导出ase色板(有色板)~
		30:点击导出ase色板(导出完成)~
		"
		/>
		<attribute name="timeStamp" type="longstring" comment="当前本地时间"/>
		<attribute name="macId" type="longstring" comment="用户mac地址"/>
	</sendMessage>
	
	<!--
		0:屏幕截图
		1:开启屏幕取色
		2:关闭屏幕取色
		3:开启网页截图分析
		4:关闭网页截图分析
		5:开启url监听
		6:关闭url监听
	-->
	<sendMessage id="50004" type="CS_SEND_USER_BEHAVIOR_SUSPENSION_WINDOW" name="CSUserBehaviorSuspensionWindowRequest" comment="悬浮窗用户行为采集">
		<attribute name="type" type="short" comment="
		类型
		0:屏幕截图
		1:开启屏幕取色
		2:关闭屏幕取色
		3:开启网页截图分析
		4:关闭网页截图分析
		5:开启url监听
		6:关闭url监听"
		/>
		<attribute name="timeStamp" type="longstring" comment="当前本地时间"/>
		<attribute name="macId" type="longstring" comment="用户mac地址"/>
	</sendMessage>
	
	
	<sendMessage id="50006" type="CS_SEND_USER_BEHAVIOR_SETTING_WINDOW" name="CSUserBehaviorSettingWindowRequest" comment="设置面板用户行为采集">
		<attribute name="launchAfterBoot" type="byte" comment="开机启动，0关闭，1开启"/>
		<attribute name="defaultPath" type="longstring" comment="默认保存路径"/>
		<attribute name="colorType" type="byte" comment="取色类型，0:hex,1:#+hex,2:rgb"/>
		<attribute name="language" type="longstring" comment="语言"/>
		<attribute name="loadOverTime" type="int" comment="加载超时时间"/>
		<attribute name="loadRefreshTime" type="int" comment="加载刷新时间"/>
		<attribute name="colorBeeLevel" type="short" comment="蜂巢级别"/>
		<attribute name="colorBeeSize" type="short" comment="蜂巢尺寸"/>
		<attribute name="paletteHueLevel" type="short" comment="色板色相级别"/>
		<attribute name="paletteSaturationLevel" type="short" comment="色板饱和度级别"/>
		<attribute name="paletteBrightnessLevel" type="short" comment="色板明度级别"/>
		<attribute name="timeStamp" type="longstring" comment="当前本地时间"/>
		<attribute name="macId" type="longstring" comment="用户mac地址"/>
	</sendMessage>
	
	<!--
	0:点击第1个人
	1:点击第2个人
	2:点击第3个人
	3:点击第4个人
	-->
	<sendMessage id="50008" type="CS_SEND_USER_BEHAVIOR_ABOUT_WINDOW" name="CSUserBehaviorAboutWindowRequest" comment="关于窗口用户行为采集">
		<attribute name="type" type="short" comment="
		类型	
		0:点击第1个人
		1:点击第2个人
		2:点击第3个人
		3:点击第4个人"
		/>
		<attribute name="state" type="byte" comment="0:点击头像，1:点击微博链接"/>
		<attribute name="timeStamp" type="longstring" comment="当前本地时间"/>
		<attribute name="macId" type="longstring" comment="用户mac地址"/>
	</sendMessage>
	
	<!--帮助页面用户行为采集-->
	<sendMessage id="50010" type="CS_SEND_USER_BEHAVIOR_HELP_WINDOW" name="CSUserBehaviorHelpWindowRequest" comment="帮助窗口用户行为采集">
		<List name="holdTimeList" type="HoldTimeVO" comment="停留时间数据包">
			<attribute name="holdTime" type="int" comment="页面停留时间"/>
		</List>
		<attribute name="checkLaunch" type="byte" comment="是否开软件的时候启动帮助窗口,1:开启,0:不开启"/>
		<attribute name="timeStamp" type="longstring" comment="当前本地时间"/>
		<attribute name="macId" type="longstring" comment="用户mac地址"/>
	</sendMessage>
</root>